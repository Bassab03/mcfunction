{
  "scopeName": "source.mcfunction",
  "name": "MCFunction",
  "fileTypes": ["mcfunction"],
  "patterns": [
    {
      "match": "@[paers](\\[(.*=.*)+\\])?",
      "name": "mcfunction.selector"
    }, {
      "match": "^(advancement|blockdata|clear|clone|defaultgamemode|difficulty|effect|enchant|entitydata|execute|fill|funtion|gamemode|gamerule|give|kill|locate|particle|playsound|recipe|reload|replaceitem|say|setblock|setworldspawn|spreadplayers|stats|stopsound|summon|teleport|tell|tellraw|testfor|testforblock|testforblocks|time|title|toggledownfall|tp|trigger|weather|worldboarder|xp)",
      "name": "mcfunction.command"
    }, {
      "match": "#.*",
      "name": "mcfunction.comment"
    }, {
      "match": "( ((~(-?\\d+(\\.\\d+)?)?)|(-?\\d+(\\.\\d+)?))){3}",
      "name": "mcfunction.coord"
    }
  ]
}
